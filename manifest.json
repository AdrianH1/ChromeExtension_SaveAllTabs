{
    "manifest_version": 2,
    "name": "Chrome Save All Tabs",
    "permissions": [
        "tabs",
        "bookmarks"
    ],
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "asdf.jpg"
    }
}